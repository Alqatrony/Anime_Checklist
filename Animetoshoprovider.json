{
  "animetosho": {
    "name": "Anime Tosho",
    "enabled": true,
    "languages": [
      "en"
    ],
    "base_url": "https://animetosho.org",
    "fallback_urls": [],
    "response_type": "text",
    "movie": {
      "query": "/search?q={query}&qx=1",
      "keywords": "{title}"
    },
    "episode": {
      "query": "/search?q={query} \"- {episode}\"|\"S{season}E{episode}\"&qx=1",
      "keywords": "{title} {absoluteNumber}"
    },
    "season": {
      "query": "/search?q={query} \"Season {season}\"&qx=1",
      "keywords": [
        "{title} {seasonCode}",
        "{title} season"
      ]
    },
    "anime": {
      "query": "/search?q={query}&qx=1",
      "keywords": "{title} {absoluteNumber}"
    },
    "separator": "+",
    "html_parser": {
      "row": "soup.find('div', id='content').find_all('div', class_='home_list_entry')",
      "title": "row.find('div', class_='link').a.text",
      "peers": "N/A",
      "seeds": "N/A",
      "size": "row.find('div', class_='size').text",
      "url": "row.find('a', {'href': re.compile('(magnet:)+[^']*')}).get('href')"
    },
    "source_is_in_sub_page": false,
    "title_replacement": {
      "'s": "s",
      "\"": ""
    }
  }
}
