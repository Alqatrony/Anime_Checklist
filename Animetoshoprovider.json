{
  "sources": {
    "nyaa": {
      "name": "Nyaa",
      "enabled": true,
      "base_url": "https://nyaa.si",
      "query_patterns": {
        "episode": "/?f=0&c=1_2&q={query}",
        "movie": "/?f=0&c=1_2&q={query}"
      },
      "language_preference": ["Sub", "Dub"],
      "resolution_preference": ["4K", "1080p", "720p", "SD"],
      "filter_settings": {
        "max_resolution": "1080p",
        "exclude_hevc": true
      }
    },
    "animetosho": {
      "name": "AnimeTosho",
      "enabled": true,
      "base_url": "https://animetosho.org",
      "query_patterns": {
        "episode": "/search?q={query}",
        "movie": "/search?q={query}"
      },
      "language_preference": ["Sub", "Dub"],
      "resolution_preference": ["4K", "1080p", "720p", "SD"],
      "filter_settings": {
        "max_resolution": "1080p",
        "exclude_hevc": true
      }
    },
    "gogoanime": {
      "name": "Gogoanime",
      "enabled": true,
      "base_url": "https://gogoanime.ai",
      "query_patterns": {
        "episode": "/search.html?keyword={query}",
        "movie": "/search.html?keyword={query}"
      },
      "language_preference": ["Sub", "Dub"],
      "resolution_preference": ["1080p", "720p", "SD"],
      "filter_settings": {
        "max_resolution": "1080p",
        "exclude_non_dub": false
      }
    }
  },
  "global_settings": {
    "language_preference": "Sub",
    "max_resolution": "1080p",
    "exclude_hevc": true,
    "sort_preference": "resolution",
    "debrid_services": {
      "real_debrid": {
        "enabled": true,
        "priority": 1
      },
      "premiumize": {
        "enabled": false,
        "priority": 2
      }
    }
  }
}
