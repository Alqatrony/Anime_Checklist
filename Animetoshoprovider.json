{
  "animetosho": {
    "name": "AnimeTosho",
    "enabled": true,
    "languages": [
      "en"
    ],
    "base_url": "https://animetosho.org",
    "fallback_urls": [],
    "response_type": "text",
    "movie": {
      "query": "/search?q={query}&aids={anidb}&qf=0",
      "keywords": "{title} {year}"
    },
    "episode": {
      "query": "/search?q={query}&aids={anidb}&qf=0",
      "keywords": "{title} {absoluteNumber}|\"S{season}E{absoluteNumber}\""
    },
    "season": {
      "query": "/search?q={query}&aids={anidb}&qf=0",
      "keywords": [
        "{title} {seasonCode}",
        "{title} season"
      ]
    },
    "anime": {
      "query": "/search?q={query}&aids={anidb}&qf=1",
      "keywords": "{title} {absoluteNumber}"
    },
    "separator": "+",
    "html_parser": {
      "row": "doc.querySelectorAll('div.home_list_entry')",
      "title": "row.querySelector('div.link a').textContent",
      "magnet": "row.querySelector('a[href^=\"magnet:\"]').getAttribute('href')",
      "size": "row.querySelector('div.size').textContent",
      "torrent": "row.querySelector('a.dllink').getAttribute('href')"
    },
    "source_is_in_sub_page": false,
    "title_replacement": {
      "'s": "s",
      "\"": ""
    }
  }
}
