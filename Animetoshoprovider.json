{
    "name": "AnimeTosho",
    "enabled": true,
    "languages": [
        "en",
        "ja"
    ],
    "base_url": "https://animetosho.org",
    "fallback_urls": [],
    "response_type": "html",
    "time_to_wait_between_each_request_ms": 500,
    "time_to_wait_on_too_many_request_ms": 2000,
    "trust_results": true,
    "token": {
        "query": "",
        "token_validity_time_ms": 0,
        "token_format": {}
    },
    "movie": {
        "query": "/search?q={query}&qx=1",
        "keywords": "{query}"
    },
    "episode": {
        "query": "/search?q={query}+\"S{season}E{episode}\"&qx=1",
        "keywords": "{title} {episodeCode}"
    },
    "season": {
        "query": "/search?q={query}+\"Season {season}\"&qx=1",
        "keywords": [
            "{title} {seasonCode}",
            "{title} season"
        ]
    },
    "json_format": {
        "results": "html_parsing",
        "url": "magnet",
        "title": "name",
        "seeds": "N/A",
        "peers": "N/A",
        "size": "size"
    },
    "title_replacement": {
        "'s": "s",
        "\"": ""
    }
}
